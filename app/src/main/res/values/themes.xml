<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Myproject" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#008EFF</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#008EFF</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="DynamicTheme1" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/color_blue</item>
        <item name="colorPrimaryDark">@color/color_blue</item>
        <item name="colorSecondary">@color/color_blue</item>
    </style>
    <style name="DynamicTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/color_pink</item>
        <item name="colorPrimaryDark">@color/color_pink</item>
        <item name="colorSecondary">@color/color_pink</item>
    </style>
    <style name="DynamicTheme3" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/color_blue</item>
        <item name="colorPrimaryDark">@color/color_blue</item>
        <item name="colorSecondary">@color/color_blue</item>
    </style>
    <style name="DynamicTheme4" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/color_blue</item>
        <item name="colorPrimaryDark">@color/color_blue</item>
        <item name="colorSecondary">@color/color_blue</item>
    </style>
</resources>